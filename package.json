{
  "name": "capita",
  "version": "1.0.1",
  "description": "cAPIta - Capitalization API powered by capstring",
  "type": "module",
  "main": "index.js",
  "exports": {
    ".": {
      "import": "./index.js"
    }
  },
  "scripts": {
    "start": "node index.js",
    "demo": "node demo.js",
    "test": "vitest run",
