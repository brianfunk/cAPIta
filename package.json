  "repository": {
    "type": "git",
    "url": "git+https://github.com/brianfunk/cAPIta.git"
  },
  "keywords": [
    "capita",
    "api",
    "cap",
    "capitalization",
    "capitalize",
    "string",
    "capstring",
    "rest",
    "kebab-case",
    "camelCase",
    "snake_case",
    "title-case",
    "text-transform",
    "spell-check"
  ],
  "author": "Brian Funk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brianfunk/cAPIta/issues"
  },
  "homepage": "https://github.com/brianfunk/cAPIta#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "capstring": "github:brianfunk/capstring#dev",
    "dictionary-en": "^4.0.0",
    "dotenv": "^16.4.0",
    "express": "^4.21.0",
    "nspell": "^2.1.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.17.0",
    "supertest": "^7.0.0",
    "vitest": "^4.0.18"
  }
